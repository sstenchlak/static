@prefix rembi: <http://www.ebi.ac.uk/bioimage-archive/ontology/rembi#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .

### Ontology declaration
rembi: a owl:Ontology ;
    rdfs:comment "OWL ontology representing the REMBI metadata model (version 1.5)" .

### Classes
rembi:REMBIStudy        rdf:type owl:Class .
rembi:Study             rdf:type owl:Class .
rembi:StudyComponent    rdf:type owl:Class .
rembi:Biosample         rdf:type owl:Class .
rembi:Specimen          rdf:type owl:Class .
rembi:ImageAcquisition  rdf:type owl:Class .
rembi:ImagingMethod     rdf:type owl:Class .
rembi:ImageCorrelation  rdf:type owl:Class .
rembi:ImageAnalysis     rdf:type owl:Class .
rembi:Annotations       rdf:type owl:Class .
rembi:Author            rdf:type owl:Class .
rembi:OrganisationURL   rdf:type owl:Class .
rembi:OrganisationInfo  rdf:type owl:Class .
rembi:License           rdf:type owl:Class .
rembi:Funding           rdf:type owl:Class .
rembi:GrantReference    rdf:type owl:Class .
rembi:Publication       rdf:type owl:Class .
rembi:Link              rdf:type owl:Class .
rembi:Organism          rdf:type owl:Class .
rembi:AnnotationType    rdf:type owl:Class .
rembi:FileLevelMetadata rdf:type owl:Class .

### Object properties

rembi:hasStudy rdf:type owl:ObjectProperty ;
    rdfs:domain rembi:REMBIStudy ;
    rdfs:range  rembi:Study .

rembi:hasStudyComponent rdf:type owl:ObjectProperty ;
    rdfs:domain rembi:REMBIStudy ;
    rdfs:range  rembi:StudyComponent .

rembi:hasBiosample rdf:type owl:ObjectProperty ;
    rdfs:domain rembi:REMBIStudy ;
    rdfs:range  rembi:Biosample .

rembi:hasSpecimen rdf:type owl:ObjectProperty ;
    rdfs:domain rembi:REMBIStudy ;
    rdfs:range  rembi:Specimen .

rembi:hasImageAcquisition rdf:type owl:ObjectProperty ;
    rdfs:domain rembi:REMBIStudy ;
    rdfs:range  rembi:ImageAcquisition .

rembi:hasImageCorrelation rdf:type owl:ObjectProperty ;
    rdfs:domain rembi:REMBIStudy ;
    rdfs:range  rembi:ImageCorrelation .

rembi:hasImageAnalysis rdf:type owl:ObjectProperty ;
    rdfs:domain rembi:REMBIStudy ;
    rdfs:range  rembi:ImageAnalysis .

rembi:hasAnnotations rdf:type owl:ObjectProperty ;
    rdfs:domain rembi:REMBIStudy ;
    rdfs:range  rembi:Annotations .

rembi:hasAuthor rdf:type owl:ObjectProperty ;
    rdfs:domain rembi:Study ;
    rdfs:range  rembi:Author .

rembi:hasLicense rdf:type owl:ObjectProperty ;
    rdfs:domain rembi:Study ;
    rdfs:range  rembi:License .

rembi:hasFunding rdf:type owl:ObjectProperty ;
    rdfs:domain rembi:Study ;
    rdfs:range  rembi:Funding .

rembi:hasPublication rdf:type owl:ObjectProperty ;
    rdfs:domain rembi:Study ;
    rdfs:range  rembi:Publication .

rembi:hasLink rdf:type owl:ObjectProperty ;
    rdfs:domain rembi:Study ;
    rdfs:range  rembi:Link .

rembi:hasOrganisationURL rdf:type owl:ObjectProperty ;
    rdfs:domain rembi:Author ;
    rdfs:range  rembi:OrganisationURL .

rembi:hasOrganisationInfo rdf:type owl:ObjectProperty ;
    rdfs:domain rembi:Author ;
    rdfs:range  rembi:OrganisationInfo .

rembi:hasGrantReference rdf:type owl:ObjectProperty ;
    rdfs:domain rembi:Funding ;
    rdfs:range  rembi:GrantReference .

rembi:hasAuthorInAnnotation rdf:type owl:ObjectProperty ;
    rdfs:domain rembi:Annotations ;
    rdfs:range  rembi:Author .

rembi:hasFileMetadata rdf:type owl:ObjectProperty ;
    rdfs:domain rembi:Annotations ;
    rdfs:range  rembi:FileLevelMetadata .

rembi:hasAnnotationType rdf:type owl:ObjectProperty ;
    rdfs:domain rembi:Annotations , rembi:FileLevelMetadata ;
    rdfs:range  rembi:AnnotationType .

rembi:hasOrganism rdf:type owl:ObjectProperty ;
    rdfs:domain rembi:Biosample ;
    rdfs:range  rembi:Organism .

rembi:hasImagingMethod rdf:type owl:ObjectProperty ;
    rdfs:domain rembi:ImageAcquisition ;
    rdfs:range  rembi:ImagingMethod .

