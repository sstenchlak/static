@prefix rembi: <http://example.org/rembi#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .

################################################################################
# Ontology declaration
################################################################################

rembi: a owl:Ontology .

################################################################################
# Classes
################################################################################

# Top‐level REMBI submission container
rembi:REMBIStudy
    a           owl:Class ;
    rdfs:label  "REMBIStudy" .

# General study information
rembi:Study
    a           owl:Class ;
    rdfs:label  "Study" ;
    rdfs:comment "General study information." .

rembi:Author
    a           owl:Class ;
    rdfs:label  "Author" .

rembi:OrganisationURL
    a           owl:Class ;
    rdfs:label  "OrganisationURL" .

rembi:OrganisationInfo
    a           owl:Class ;
    rdfs:label  "OrganisationInfo" .

# The license under which the data are available.
rembi:License
    a           owl:Class ;
    rdfs:label  "License" ;
    rdfs:comment "The license under which the data are available." .

rembi:Funding
    a           owl:Class ;
    rdfs:label  "Funding" .

rembi:GrantReference
    a           owl:Class ;
    rdfs:label  "GrantReference" .

rembi:Publication
    a           owl:Class ;
    rdfs:label  "Publication" .

rembi:Link
    a           owl:Class ;
    rdfs:label  "Link" .

rembi:StudyComponent
    a           owl:Class ;
    rdfs:label  "StudyComponent" .

rembi:Biosample
    a           owl:Class ;
    rdfs:label  "Biosample" .

# Species.
rembi:Organism
    a           owl:Class ;
    rdfs:label  "Organism" ;
    rdfs:comment "Species." .

# How the sample was grown or cultured and prepared for imaging.
rembi:Specimen
    a           owl:Class ;
    rdfs:label  "Specimen" ;
    rdfs:comment "How the sample was grown or cultured and prepared for imaging." .

rembi:ImageAcquisition
    a           owl:Class ;
    rdfs:label  "ImageAcquisition" .

# What method was used to capture images.
rembi:ImagingMethod
    a           owl:Class ;
    rdfs:label  "ImagingMethod" ;
    rdfs:comment "What method was used to capture images." .

# How images from the same correlative study are linked
rembi:ImageCorrelation
    a           owl:Class ;
    rdfs:label  "ImageCorrelation" ;
    rdfs:comment "How images from the same correlative study are linked." .

rembi:ImageAnalysis
    a           owl:Class ;
    rdfs:label  "ImageAnalysis" .

# A set of annotations for an AI‐ready dataset
rembi:Annotations
    a           owl:Class ;
    rdfs:label  "Annotations" ;
    rdfs:comment "A set of annotations for an AI‐ready dataset." .

# Stub class for annotation types (no further details provided)
rembi:AnnotationType
    a           owl:Class ;
    rdfs:label  "AnnotationType" .

# Metadata attributes that must be detailed at the file level
rembi:FileLevelMetadata
    a           owl:Class ;
    rdfs:label  "FileLevelMetadata" ;
    rdfs:comment "Metadata attributes that must be detailed at the file level." .

################################################################################
# Object Properties (associations)
################################################################################

# REMBIStudy → Study
rembi:study
    a           owl:ObjectProperty ;
    rdfs:label  "study" ;
    rdfs:domain rembi:REMBIStudy ;
    rdfs:range  rembi:Study .

# REMBIStudy → StudyComponent
rembi:study_components
    a           owl:ObjectProperty ;
    rdfs:label  "study components" ;
    rdfs:domain rembi:REMBIStudy ;
    rdfs:range  rembi:StudyComponent .

# REMBIStudy → Biosample
rembi:sample
    a           owl:ObjectProperty ;
    rdfs:label  "sample" ;
    rdfs:domain rembi:REMBIStudy ;
    rdfs:range  rembi:Biosample .

# REMBIStudy → Specimen
rembi:specimen
    a           owl:ObjectProperty ;
    rdfs:label  "specimen" ;
    rdfs:domain rembi:REMBIStudy ;
    rdfs:range  rembi:Specimen .

# REMBIStudy → ImageAcquisition
rembi:image_acquisition
    a           owl:ObjectProperty ;
    rdfs:label  "image acquisition" ;
    rdfs:domain rembi:REMBIStudy ;
    rdfs:range  rembi:ImageAcquisition .

# REMBIStudy → ImageCorrelation
rembi:image_correlation
    a           owl:ObjectProperty ;
    rdfs:label  "image correlation" ;
    rdfs:domain rembi:REMBIStudy ;
    rdfs:range  rembi:ImageCorrelation .

# REMBIStudy → ImageAnalysis
rembi:image_analysis
    a           owl:ObjectProperty ;
    rdfs:label  "image analysis" ;
    rdfs:domain rembi:REMBIStudy ;
    rdfs:range  rembi:ImageAnalysis .

# REMBIStudy → Annotations
rembi:annotations
    a           owl:ObjectProperty ;
    rdfs:label  "annotations" ;
    rdfs:domain rembi:REMBIStudy ;
    rdfs:range  rembi:Annotations .

# Study → Author
rembi:authors
    a           owl:ObjectProperty ;
    rdfs:label  "authors" ;
    rdfs:domain rembi:Study ;
    rdfs:range  rembi:Author .

# Study → License
rembi:license
    a           owl:ObjectProperty ;
    rdfs:label  "license" ;
    rdfs:domain rembi:Study ;
    rdfs:range  rembi:License .

# Study → Funding
rembi:funding
    a           owl:ObjectProperty ;
    rdfs:label  "funding" ;
    rdfs:domain rembi:Study ;
    rdfs:range  rembi:Funding .

# Study → Publication
rembi:publications
    a           owl:ObjectProperty ;
    rdfs:label  "publications" ;
    rdfs:domain rembi:Study ;
    rdfs:range  rembi:Publication .

# Study → Link
rembi:links
    a           owl:ObjectProperty ;
    rdfs:label  "links" ;
    rdfs:domain rembi:Study ;
    rdfs:range  rembi:Link .

# Funding → GrantReference
rembi:grant_references
    a           owl:ObjectProperty ;
    rdfs:label  "grant references" ;
    rdfs:domain rembi:Funding ;
    rdfs:range  rembi:GrantReference .

# Biosample → Organism
rembi:organism
    a           owl:ObjectProperty ;
    rdfs:label  "organism" ;
    rdfs:domain rembi:Biosample ;
    rdfs:range  rembi:Organism .

# ImageAcquisition → ImagingMethod
rembi:imaging_method
    a           owl:ObjectProperty ;
    rdfs:label  "imaging method" ;
    rdfs:domain rembi:ImageAcquisition ;
    rdfs:range  rembi:ImagingMethod .

# Annotations → Author
rembi:annotation_authors
    a           owl:ObjectProperty ;
    rdfs:label  "authors" ;
    rdfs:domain rembi:Annotations ;
    rdfs:range  rembi:Author .

# Annotations → FileLevelMetadata
rembi:file_metadata
    a           owl:ObjectProperty ;
    rdfs:label  "file metadata" ;
    rdfs:domain rembi:Annotations ;
    rdfs:range  rembi:FileLevelMetadata .

# Annotations → AnnotationType
rembi:annotation_type
    a           owl:ObjectProperty ;
    rdfs:label  "annotation type" ;
    rdfs:domain rembi:Annotations , rembi:FileLevelMetadata ;
    rdfs:range  rembi:AnnotationType .

